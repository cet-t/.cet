	"prettier.requireConfig": true,
	"security.workspace.trust.untrustedFiles": "open",
	"githubPullRequests.createOnPublishBranch": "never",
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"git.autofetch": true,
	"git.useEditorAsCommitInput": false,
	"git.openRepositoryInParentFolders": "always",
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	"files.encoding": "utf8bom",
	// "files.encoding": "utf8",
	"python.analysis.inlayHints.functionReturnTypes": true,
	"python.analysis.inlayHints.variableTypes": true,
	"C_Cpp.formatting": "clangFormat",
	"workbench.iconTheme": "vscode-great-icons",
	"workbench.startupEditor": "none",
	"python.defaultInterpreterPath": "C:\\Users\\cet\\AppData\\Local\\Programs\\Python\\Python310\\python.exe",
	// "python.formatting.provider": "none",
	// "python.formatting.provider": "black",
	// "python.formatting.provider": "autopep8",
	"[python]": {
		"editor.defaultFormatter": "ms-python.autopep8"
	},
	"[csharp]": {
		"editor.defaultFormatter": "ms-dotnettools.csharp"
},
"C_Cpp.default.compilerPath": "cl.exe",
	"[cpp]": {
		"editor.defaultFormatter": "ms-vscode.cpptools"
	},
	"redhat.telemetry.enabled": true,
	"update.showReleaseNotes": false,
	"errorLens.fontSize": "m plus 1 code regular",
	"debug.console.fontFamily": "m plus 1 code regular",
	"notebook.output.fontFamily": "m plus 1 code regular",
	"chat.editor.fontFamily": "m plus 1 code regular",
	"editor.codeLensFontFamily": "m plus 1 code regular",
	"errorLens.fontFamily": "m plus 1 code regular",
	"editor.inlayHints.fontFamily": "m plus 1 code regular",
	"markdown.preview.fontFamily": "m plus 1 code regular",
	// gitlens settings https://kuma-emon.com/it/pc/3832/
	"gitlens.defaultDateLocale": null,
	"gitlens.defaultDateFormat": "YYYY/MM/DD HH:mm:ss",
	"gitlens.defaultTimeFormat": null,
	"gitlens.defaultDateStyle": "absolute",
	"C_Cpp.intelliSenseEngineFallback": "enabled",
	"workbench.editor.closeEmptyGroups": false,
	"glassit.alpha": 255,
	"glassit.step": 5,
	"dotnet.inlayHints.enableInlayHintsForParameters": false,
	"csharp.inlayHints.enableInlayHintsForTypes": false,
	"plantuml.exportFormat": "png",
	"plantuml.render": "PlantUMLServer",
	"plantuml.server": "https://www.plantuml.com/plantuml",
	"dotnet.codeLens.enableReferencesCodeLens": false,
	"dotnet.codeLens.enableTestsCodeLens": false,
	"material-icon-theme.folders.theme": "specific",
	"workbench.colorTheme": "Atom One Dark",
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"lldb.suppressUpdateNotifications": true,
}
